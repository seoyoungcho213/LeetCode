bool canConstruct(char * ransomNote, char * magazine){
    int stack[26];
    
    while(*ransomNote){
        map[*ransomNote - 'a']++;
        ransomNote++;
    }
    while(*magazine){
        map[*magazine - 'a']--;
        magazine++;
    }

    for(int i=0; i < 26; i++)
        if (map[i] > 0) return false;
    return true;
}
